<%- model_class = Entries -%>
<div class="row">
  <div class="col-xs-12">
    <div class="page-header">
      <h3>Latest News</h3>
    </div>

    <div class="col-xs-4">
      <div class="panel panel-default">
        <div class="panel-body">
          <h4 class="media-heading"><a href="<%= @entry.url %>" title="<%= @entry.title %>"><%= @entry.title %></a></h4>
          <p> <%= @entry.content %></p>
          <span class="label label-default"> </span>
        </div>
      </div>
    </div>  
    <% end %>
  </div>
</div>